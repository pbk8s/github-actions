#!/usr/bin/expect -f
set force_conservative 0 ;# set to 1 to force conservative mode even if ;# script wasn't run conservatively originally
if {$force_conservative} {
set send_slow {1 .1}
proc send {ignore arg} { sleep .1
exp_send -s -- $arg
}
}
set TOKEN [lindex $argv 0]
set timeout -1
spawn ./config.sh --url https://github.com/armisv/demo --token $TOKEN match_max 100000
expect "Enter the name of runner:"
send -- "\r"
expect "This runner will have the following labels:"
send -- "\r"
expect "Enter name of work folder:"
send -- "\r"
expect eof
